# ClawPwn environment configuration
# Copy to .env and fill in values as needed.

# Primary LLM provider (e.g., openai, anthropic, local)
CLAWPWN_LLM_PROVIDER=

# API key or token for the selected provider
CLAWPWN_LLM_API_KEY=

# Base URL for self-hosted providers (optional)
CLAWPWN_LLM_BASE_URL=

# Default model name (e.g., gpt-4.1, claude-3.5-sonnet)
CLAWPWN_LLM_MODEL=

# Path where project data will be stored (optional)
CLAWPWN_DATA_DIR=

# Enable/disable verbose logging (true/false)
CLAWPWN_VERBOSE=
